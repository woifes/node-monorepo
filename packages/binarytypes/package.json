{
  "name": "@woifes/binarytypes",
  "version": "1.0.0",
  "description": "Handling of binary datatypes",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "test": "jest",
    "compile": "tsc -p tsconfig.build.json",
    "prettier": "prettier -w ./src/**/*.ts ./test/**/*.ts ./index.ts",
    "lint": "eslint ./src/ ./index.ts ./test/ --fix",
    "lint:check": "eslint ./src/ ./index.ts ./test/",
    "prepare": "pnpm compile",
    "licenseReport": "node ../../node_modules/@woifes/license-report/dist/index.js"
  },
  "keywords": [
    "Binary",
    "Integer",
    "binary",
    "INT8",
    "UINT8",
    "INT16",
    "UINT16",
    "INT32",
    "UINT32",
    "INT64",
    "UINT64",
    "FLOAT",
    "DOUBLE"
  ],
  "author": "woifes",
  "license": "MIT",
  "dependencies": {
    "runtypes": "^6.5.1"
  }
}
